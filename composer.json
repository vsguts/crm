{
    "name": "vsguts/crm",
    "description": "CRM",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://vsguts.ru/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-twig": "^2.1",
        "yiisoft/yii2-queue": "~2.0",
        "yiidoc/yii2-redactor": "*",
        "mero/yii2-monolog": "^0.2.0@dev",
        "yurkinx/yii2-image": "1.*",
        "boundstate/yii2-htmlconverter": "*",
        "phpoffice/phpexcel": "*",
        "bower-asset/jquery.cookie": "1.4.*",
        "bluezed/yii2-float-thead": "*",
        "kartik-v/yii2-widgets": "3.4.*",
        "kartik-v/yii2-widget-select2": "1.*",
        "kartik-v/yii2-field-range": "1.3.*",
        "2amigos/yii2-gallery-widget": "1.0.*",
        "2amigos/yii2-google-maps-library" : "0.1.*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
